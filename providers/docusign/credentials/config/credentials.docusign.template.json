  "redirect_uri": "http://localhost",

  "protocol": "https",

  "auth_account_env_hqtest3": "account-hqtest3",
  "auth_account_env_hqtest": "account-hqtest",
  "auth_account_env_stage": "account-s",
  "auth_account_env_demo": "account-d",
  "auth_account_env_prod": "account",

