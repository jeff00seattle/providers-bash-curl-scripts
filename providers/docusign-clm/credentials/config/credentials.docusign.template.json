{
  "client_id": "[** CLIENT_ID **]",
  "client_secret": "[** CLIENT_SECRET **]",
  "redirect_uri": "http://localhost",
  "protocol": "https",
  "auth_domain_env": "account-d",
  "auth_domain": "docusign.com",
  "auth_url": "${PROTOCOL}://${AUTH_DOMAIN_ENV}.${AUTH_DOMAIN}/oauth/auth",
  "auth_token_url": "${PROTOCOL}://${AUTH_DOMAIN_ENV}.${AUTH_DOMAIN}/oauth/token",
  "auth_user_url": "${PROTOCOL}://${AUTH_DOMAIN_ENV}.${AUTH_DOMAIN}/oauth/userinfo",
  "auth_scope": "spring_read spring_write",
  "docusign_api_version": "v2.1"
}
